<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <style>
        img{
            width: 100px;
        }
    </style>
    <title>Document</title>
</head>
<body>
<ul>
    <% for (var i = 0; i < listData.length; i++){ %>
        <li class="data-item">
        <span><img src="http://res.cloudinary.com/a123abc/image/upload/<%= listData[i].imageUrl %>"
                   alt="<%= listData[i].name %>"></span>
            <span><%= listData[i].name %></span>
            <span>

                <a href="/event/edit/<%= listData[i]._id %>">Edit</a>&nbsp;

        </span>

        </li>
    <% } %>
</ul>
</body>
</html>